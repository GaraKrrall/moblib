{
  "schemaVersion": 1,
  "id": "moblib",
  "version": "${version}",
  "name": "MobLiB",
  "description": "Go, go, go!",
  "authors": [],
  "contact": {},
  "license": "AGPL-3.0",
  "icon": "assets/moblib/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "com.kaplanlib.client.moblibDataGenerator"
    ],
    "client": [
      "com.kaplanlib.client.moblibClient"
    ],
    "main": [
      "com.kaplanlib.moblib"
    ]
  },
  "mixins": [
    "moblib.mixins.json",
